{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    //connection string of communication resource
    "ResourceConnectionString": "%ResourceConnectionString%",
    //web pubsub URI
    "MediaStreamingTransportURI": "%MediaStreamingTransportURI%",
    //url of the deployed API
    "AppCallBackUri": "%AppCallBackUri%",
    //public url of wav audio
    "AudioFileUri": "%AudioFileUri%",
    //Query string for callback URL
    "SecretValue": "%SecretValue%",
    //Target participant phone number (Number from which we are calling)
    "TargetParticipant": "%TargetParticipant%",
    // Ngrok Exe Path
    "NgrokPath": "%NgrokExePath%"
}
